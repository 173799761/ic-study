type wasm_module = vec nat8;
type canister_id = principal;
service : {
  create_canister: () -> (canister_id);
  delete_canister: (canister_id) -> ();
  install_code:
   (record {
      canister_id: canister_id;
      mode: variant {
              install;
              reinstall;
              upgrade;
            };
      wasm: wasm_module;
    }) -> ();
  start_canister: (canister_id) -> ();
  stop_canister: (canister_id) -> ();
}
